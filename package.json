  "dependencies": {
    "nw": "^0.32.4-sdk",
    "parcel-bundler": "^1.9.7",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "nw-builder": "^3.5.4"
  },
  "window": {
